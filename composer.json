{
    "name": "jguittard/zpress",
    "description": "Zend Framework 2 based blog engine",
    "type": "application",
    "license": "BSD-3-Clause",
    "keywords": [
        "blog",
        "framework",
        "zf2"
    ],
    "authors": [
        {
            "name": "Julien Guittard",
            "email": "julien.guittard@me.com"
        }
    ],
    "homepage": "http://zpress.io",
    "support": {
        "email": "support@zpress.io",
        "source": "https://github.com/jguittard/zpress",
        "issues": "https://github.com/jguittard/zpress/issues"
    },
    "require": {
        "zendframework/zendframework": "~2.5",
        "zfcampus/zf-development-mode": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5.0",
        "squizlabs/php_codesniffer": "~2.3.4"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        },
        "classmap": [
            "module/Application/Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    }
}
